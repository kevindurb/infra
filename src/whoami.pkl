import "@k8s/K8sResource.pkl"
import "./lib/Application.pkl"

app: Application = new {
  name = "whoami"
  image = "docker.io/traefik/whoami:v1.10.2"
  port = 80
  ingressType = "lan"
}

output {
  value = app.getResources()
  renderer = (K8sResource.output.renderer as YamlRenderer) {
    isStream = true
  }
}
